{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "PetSearchOpenApiInfo": {
    "Title": "Pet Search",
    "Description": "An ASP.NET Core Web API for searching pets for adoption within an area location.",
    "Version": "1.0",
    "ClientAppName": "Pet Search Client UI Application",
    "ClientAppLink": "https://pet-search-angular.vercel.app"
  },
  "PetFinderSettings": {
    "BaseUri": "https://api.petfinder.com/v2/",
    "PetSearchUrl": "animals",
    "TokenUrl": "oauth2/token"
  },
  "MapBoxSettings": {
    "Uri": "https://api.mapbox.com/search/geocode/v6/",
    "Options": {
      "language": "en",
      "country": "us",
      "types": "postcode",
      "limit": 1
    }
  },
  "AllowedHosts": "*"
}
